{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [{
      "title": "Pages",
      "path": "[[workspace]]/.frontmatter/content",
      "excludeSubdir": true,
      "contentTypes": ["post"]
    },
    {
      "title": "Blog posts",
      "path": "[[workspace]]/.frontmatter/content/posts",
      "contentTypes": ["post"]
    },
    {
      "title": "speaking",
      "path": "[[workspace]]/.frontmatter/content/speaking",
      "disableCreation": true
    },
    {
      "title": "sessions",
      "path": "[[workspace]]/.frontmatter/content/session",
      "contentTypes": ["session"],
      "disableCreation": true
    }
  ]
}
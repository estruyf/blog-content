{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.logging": "verbose",
  "frontMatter.copilot.family": "gpt-4-turbo",
  "frontMatter.website.host": "https://www.eliostruyf.com",
  "frontMatter.framework.startCommand": "hugo server -D",
  "frontMatter.content.fmHighlight": true,
  "frontMatter.global.activeMode": "",
  "frontMatter.global.modes": [
    {
      "id": "minimal",
      "features": [
        "panel.metadata",
        "panel.globalSettings",
        "panel.seo",
        "dashboard.data.view",
        "dashboard.snippets.view"
      ]
    }
  ],
  "frontMatter.experimental": true,
  "frontMatter.extensibility.scripts": [
    "[[workspace]]/.frontmatter/ui/external.js"
  ],
  "frontMatter.taxonomy.quoteStringValues": true,
  "frontMatter.snippets.wrapper.enabled": false,
  "frontMatter.sponsors.ai.enabled": true,
  "frontMatter.git.enabled": true,
  "frontMatter.git.submodule.push": true,
  "frontMatter.git.submodule.pull": true,
  "frontMatter.git.submodule.branch": "main",
  "frontMatter.git.submodule.folder": ".frontmatter",
  "frontMatter.dashboard.content.pagination": true,
  "frontMatter.dashboard.contentTags": "tags",
  "frontMatter.content.autoUpdateDate": false,
  "frontMatter.content.wysiwyg": true,
  "frontMatter.content.filters": [
    "contentFolders",
    "tags",
    {
      "title": "Content Type",
      "name": "fmContentType"
    }
  ],
  "frontMatter.content.sorting": [
    {
      "title": "Date (asc)",
      "name": "date",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Date (desc)",
      "name": "date",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.content.placeholders": [
    {
      "id": "ogImage",
      "script": "./scripts/og-image.mjs",
      "command": "~/.nvm/versions/node/v18.17.1/bin/node"
    }
  ],
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.publicFolder": ".frontmatter/static",
  "frontMatter.content.defaultSorting": "DateDesc",
  "frontMatter.dashboard.openOnStart": false,
  "frontMatter.taxonomy.slugPrefix": "/",
  "frontMatter.taxonomy.slugSuffix": "/",
  "frontMatter.framework.id": "hugo",
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "customTags",
      "options": [
        "Custom"
      ]
    }
  ],
  "frontMatter.data.files": [
    {
      "id": "projects",
      "title": "Projects",
      "file": "[[workspace]]/themes/eliostruyf-v2/data/projects.json",
      "fileType": "json",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.data.folders": [
    {
      "id": "Testable",
      "path": "[[workspace]]/themes/eliostruyf-v2/data/",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.panel.actions.disabled": [
    "optimizeSlug"
  ]
}